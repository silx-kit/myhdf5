    "normalize.css": "8.0.1",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-dropzone": "14.3.8",
    "react-error-boundary": "6.0.0",
    "react-hook-form": "7.66.0",
    "react-icons": "5.4.0",
    "react-router-dom": "6.3.0",
    "suspend-react": "0.1.3",
    "zustand": "5.0.8"
  },
  "devDependencies": {
    "@esrf/eslint-config": "1.6.1",
    "@types/node": "^24.10.1",
    "@types/react": "^18.3.26",
    "@types/react-dom": "^18.3.7",
    "@vitejs/plugin-react-swc": "4.2.2",
    "eslint": "9.38.0",
    "prettier": "3.6.2",
    "typescript": "5.9.3",
    "vite": "7.2.2",
    "vite-plugin-checker": "0.11.0"
  }
}
